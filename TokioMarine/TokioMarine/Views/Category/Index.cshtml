@using DTO.Category
@model List<GetCategoryDto>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
@Html.ActionLink("Create New Category","Create")
<h1></h1>

  <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
    </tr>
  </thead>
  <tbody>
        @foreach (var item in @Model)
        {
            <tr>
             <th scope="row">@item.Id</th>
             <td>@item.Name</td>
          </tr>
        }

  </tbody>
</table>
   

